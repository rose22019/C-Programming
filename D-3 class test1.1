#include <stdio.h>

int main() {
   int i, j, k;
   for (i = 0; i <= 10; i++) {
      for (j = 0; j <= 10 - i; j++) {
         printf("%d", j);
      }
      for (k = 0; k < i; k++) {
         printf("0");
      }
      printf("\n");
   }

   printf("\n");
   for (i = 10; i >= 0; i--) {
      for (j = 0; j <= 10 - i; j++) {
         printf("%d", j);
      }
      for (k = 0; k < i; k++) {
         printf("0");
      }
      printf("\n");
   }

   return 0;
}
